        <!-- ##### Quote Subscribe Area Start ##### -->
        <section class="quote-subscribe-adds">
            <div class="container">
                <div class="row align-items-end">
                    <!-- Quote -->
                    <div class="col-12 col-lg-4">
                        <div class="quote-area text-center">
                            <span>"</span>
                            <h4>
                                <%= data.text %>
                            </h4>
                            <span>"</span>
                            <p><%= data.author %></p>
                            <div class="date-comments d-flex justify-content-between">
                                <div class="date">January 04, 2000</div>
                                <div class="comments">Quotes</div>
                            </div>
                        </div>
                    </div>

                    <!-- Newsletter -->
                    <div class="col-12 col-lg-4">
                        <div class="newsletter-area">
                            <h4>Subscribe to our newsletter</h4>
                            <!-- Form -->
                            <div
                                class="newsletter-form bg-img bg-overlay"
                                style="background-image: url(img/bg-img/bg1.jpg);"
                            >
                                <form action="#" method="post">
                                    <input type="email" name="email" placeholder="Subscribe to newsletter" />
                                    <button type="submit" class="btn delicious-btn w-100">Subscribe</button>
                                </form>
                                <p>
                                    Don't worry, we don't sell your emil to anyone.
                                </p>
                            </div>
                        </div>
                    </div>

                    <!-- Ads -->
                    <div class="col-12 col-lg-4">
                        <div class="delicious-add">
                            <img
                                src="https://www.minnesotamonthly.com/wp-content/uploads/sites/85/2019/06/HR-new-vendor-blue-ox-burger-bar-683x1024.jpg"
                                alt=""
                                height="200px"
                            />
                        </div>
                    </div>
                </div>
            </div>
        </section>
